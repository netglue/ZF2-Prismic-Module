{
    "name" : "netglue/zf2-prismic-module",
    "description" : "ZF2 module for the Prismic PHP SDK. Simple wrapper and some added convenience.",
    "licence" : "MIT",
    "keywords" : [
        "prismic.io",
        "zf2",
        "netglue"
    ],
    "homepage" : "http://netglue.co/",
    "authors" : [
        {
            "name" : "George Steel",
            "email" : "george@net-glue.co.uk",
            "homepage" : "http://netglue.co/"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require" : {
        "php" : ">=5.4",
        "zendframework/zendframework" : "~2.3",
        "zendframework/zend-mvc" : "<=2.6.3",
        "prismic/php-sdk": ">=1.5.3"
    },
    "require-dev" : {
        "codeclimate/php-test-reporter": "dev-master",
        "phpunit/phpunit": "~5.0"
    },
    "autoload" : {
        "psr-0" : {
            "NetgluePrismic" : [ "src/", "tests/" ]
        },
		"classmap": [
			"src/"
		]
    },
    "repositories": [
    ]
}
